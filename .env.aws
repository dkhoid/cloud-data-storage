# AWS S3 Configuration
MINIO_ENDPOINT=s3.ap-southeast-1.amazonaws.com
MINIO_ACCESS_KEY=YOUR_AWS_ACCESS_KEY_ID
MINIO_SECRET_KEY=YOUR_AWS_SECRET_ACCESS_KEY
MINIO_BUCKET=your-cloud-storage-bucket

# Thêm secure=True cho AWS S3
MINIO_SECURE=True

# Database và các config khác giữ nguyên
DATABASE_URL=postgresql://postgres:password@postgres:5432/cloud_storage
JWT_SECRET=your-secure-jwt-secret-key
FLASK_ENV=production
FLASK_DEBUG=False